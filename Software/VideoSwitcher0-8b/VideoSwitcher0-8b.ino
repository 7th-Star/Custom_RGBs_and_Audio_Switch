void setup(void) {
  
  u8g2.begin();

  if (input < 0 || input > 16){
  input = 0;  
  }

  //Serial.begin(9600);

  DDRB = B00111111;
  DDRD = B00000010;
    
  lastStateCLK = digitalRead(CLK);
  
  attachInterrupt(0, Encoderchange, CHANGE);
  attachInterrupt(1, Encoderchange, CHANGE);

  bootscreen();
  displayinput();
  setinput();
}

