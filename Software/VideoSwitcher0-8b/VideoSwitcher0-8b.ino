void (*selectinput[])() = {playstation,snes,mastersystem,xbox,saturn,playstation2,genesis,kanekosupernova,openslot,loopy,nes,pc,pcengine,vcr,gamecube,laserdisc};

void loop(void) {

  currenttime = millis();
  
   if (dplayedinput != input){
    displayinput();}  
 
  if (input != currentinput && currenttime > lastdisplaychange){
    setinput();}

  if (currenttime + 1001 - lastdisplaychange > 10000){
    u8g2.firstPage();
  do {
    u8g2.setFont(u8g2_font_ncenB14_tr);
  } while ( u8g2.nextPage() ); 
  }


}

void displayinput(){

  PORTB = PORTB & B11011111;
  (*selectinput[input])();
  lastdisplaychange = currenttime + 1000;
  dplayedinput = input;
   
}

void Encoderchange(){

  currenttime = millis();

  currentStateCLK = digitalRead(CLK);
  
  if (currentStateCLK != lastStateCLK  && currentStateCLK == 1 && currenttime >= encodewait){
    
    if (digitalRead(DT) != currentStateCLK) 
    {if (input > 0) {input --;}
     else {input = 15;}}
    else 
    {if (input < 15) {input ++;}
     else {input = 0;}}
     encodewait = currenttime + 100;
  }
     lastStateCLK = currentStateCLK;
}

void setinput(){

 byte buffer5 pgm_read_byte(&demuxout[input]);
 PORTB = buffer5;
 PORTB = PORTB | B00100000;
 EEPROM.write(0, input);   
 currentinput = input;
  
}

void bootscreen(){
 
   u8g2.firstPage();
  do {
    u8g2.setFont(u8g2_font_ncenB14_tr);
    u8g2.drawXBMP(0,0, 128,64, boot_pic);
  } while ( u8g2.nextPage() );
 delay(3000);
      
}
